router eigrp {{ eigrp.as }}
{% for network in eigrp.networks %}
{% if network.mask is defined %}
 network {{ network.subnet }} {{ network.mask }}
{% else %}
 network {{ network.subnet }}
{% endif %}
{% endfor %}
 passive-interface default
{% for interface in eigrp.non_passive %}
 no passive-interface {{ interface }}
{% endfor %}
